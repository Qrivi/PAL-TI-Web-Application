        <css>https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css</css>
        <!-- jquery -->
        <js>https://code.jquery.com/jquery-2.2.0.min.js</js>
        <js>https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.js</js>
